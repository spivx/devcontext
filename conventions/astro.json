{
  "id": "astro",
  "rules": [
    {
      "if": { "toolingIncludes": ["vue"] },
      "set": { "tooling": "vue" }
    },
    {
      "if": { "toolingIncludes": ["svelte"] },
      "set": { "tooling": "svelte" }
    },
    {
      "if": { "toolingIncludes": ["ssr"] },
      "set": { "dataFetching": "ssr" }
    },
    {
      "if": { "toolingIncludes": ["hybrid"] },
      "set": { "dataFetching": "hybrid" }
    }
  ]
}
